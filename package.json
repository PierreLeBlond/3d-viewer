{
  "name": "3d-viewer",
  "version": "1.0.0",
  "description": "Personal three.js based gltf viewer",
  "engines": {
    "node": ">=14.17.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "type": "dist/index.d.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack/config.js",
    "pack": "npm pack --pack-destination ./dist",
    "start": "NODE_ENV=development webpack serve --config ./webpack/config.js",
    "clean": "rm -rf ./dist/* & ssh ssh://ncyujthp@node104-eu.n0c.com:5022 'rm -rf ./app/3d-viewer/*'",
    "deploy": "npm run clean && npm run build && npm run pack && scp -r -P 5022 ./dist/* ncyujthp@node104-eu.n0c.com:./app/3d-viewer/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "PierreLeBlond",
  "license": "ISC",
  "devDependencies": {
    "@types/three": "^0.134.0",
    "three": "^0.134.0",
    "file-loader": "^6.2.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-glsl-loader": "^1.0.1",
    "webpack-merge": "^5.8.0"
  }
